            @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form", id = "registerForm" }))
            {
                @Html.AntiForgeryToken()
                //<h4>Crear Nueva Cuenta Solo para Administradores.</h4>
                @Html.ValidationSummary()
                <div class="dl-horizontal col-md-6">
                    <fieldset class="form-group">
                        @Html.LabelFor(m => m.UserName, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @*@Html.TextBoxFor(m => m.UserName, new { @class = "form-control" })*@
                            @Html.EditorFor(m => m.UserName)
                            @Html.ValidationMessageFor(m => m.UserName)
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        @Html.LabelFor(m => m.Password, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @*@Html.PasswordFor(m => m.Password, new { @class = "form-control" })*@
                            @Html.EditorFor(m => m.Password)
                            @Html.ValidationMessageFor(m => m.Password)
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        @Html.LabelFor(m => m.ConfirmPassword, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @*@Html.PasswordFor(m => m.ConfirmPassword, new { @class = "form-control" })*@
                            @Html.EditorFor(m => m.ConfirmPassword)
                            @Html.ValidationMessageFor(m => m.ConfirmPassword)
                        </div>
                    </fieldset>

                    <fieldset class="form-group">
                        @Html.LabelFor(model => model.fullName, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @Html.EditorFor(model => model.fullName)
                            @Html.ValidationMessageFor(model => model.fullName)
                        </div>
                    </fieldset>

                    <fieldset class="form-group">
                        @Html.LabelFor(model => model.addressLine1, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @Html.EditorFor(model => model.addressLine1)
                            @Html.ValidationMessageFor(model => model.addressLine1)
                        </div>
                    </fieldset>

                    <fieldset class="form-group" id="">
                        @Html.LabelFor(model => model.addressLine2, new { @class = "control-label col-md-12" })
                        <div class="col-md-12">
                            @Html.EditorFor(model => model.addressLine2)
                            @Html.ValidationMessageFor(model => model.addressLine2)
                        </div>
                    </fieldset>
                   
                   
                        <fieldset class="form-group" id="">
                            @Html.LabelFor(model => model.city, new { @class = "control-label col-md-12" })
                            <div class="col-md-12">
                                @Html.EditorFor(model => model.city)
                                @Html.ValidationMessageFor(model => model.city)
                            </div>
                        </fieldset>
                    </div>
                    <div class="dl-horizontal col-md-6">
                        <fieldset class="form-group" id="">
